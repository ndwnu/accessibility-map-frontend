{
  "name": "accessibility-map-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --output-hashing all",
    "build:prod": "ng build --configuration production && npm run postbuild",
    "postbuild": "node scripts/replace-set-env.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:prod": "ng test --watch=false",
    "lint": "ng lint",
    "format": "npx prettier . --write",
    "format:check": "npx prettier . --check"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "18.0.1",
    "@angular/cdk": "^18.0.1",
    "@angular/common": "18.0.1",
    "@angular/compiler": "18.0.1",
    "@angular/core": "18.0.1",
    "@angular/forms": "18.0.1",
    "@angular/platform-browser": "18.0.1",
    "@angular/platform-browser-dynamic": "18.0.1",
    "@angular/router": "18.0.1",
    "@ndwnu/design-system": "^1.1.1",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@ngneat/until-destroy": "10.0.0",
    "@popperjs/core": "2.11.8",
    "@turf/bbox": "6.5.0",
    "bootstrap": "5.3.2",
    "boxicons": "^2.1.4",
    "geojson": "^0.5.0",
    "maplibre-gl": "3.5.2",
    "rxjs": "7.8.1",
    "tslib": "2.3.0",
    "zone.js": "0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "18.0.2",
    "@angular-eslint/builder": "18.0.1",
    "@angular-eslint/eslint-plugin": "18.0.1",
    "@angular-eslint/eslint-plugin-template": "18.0.1",
    "@angular-eslint/schematics": "18.0.1",
    "@angular-eslint/template-parser": "18.0.1",
    "@angular/cli": "18.0.2",
    "@angular/compiler-cli": "18.0.1",
    "@angular/localize": "18.0.1",
    "@total-typescript/ts-reset": "0.5.1",
    "@types/jasmine": "5.1.2",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@typescript-eslint/types": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "9.1.0",
    "jasmine-core": "5.1.1",
    "karma": "6.4.2",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "prettier": "3.1.1",
    "typescript": "5.4.5"
  },
  "overrides": {
    "@ng-bootstrap/ng-bootstrap": {
      "@angular/common": "18.0.1",
      "@angular/core": "18.0.1",
      "@angular/forms": "18.0.1",
      "@angular/localize": "18.0.1"
    }
  }
